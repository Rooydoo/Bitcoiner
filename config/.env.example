# API Keys（このファイルをコピーして .env を作成し、実際のキーを入力してください）

# bitFlyer API
BITFLYER_API_KEY=your_api_key_here
BITFLYER_API_SECRET=your_api_secret_here

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Streamlit認証
STREAMLIT_USERNAME=admin
STREAMLIT_PASSWORD=your_secure_password_here

# Google Drive バックアップ（オプション）
GOOGLE_DRIVE_CREDENTIALS_PATH=credentials.json
